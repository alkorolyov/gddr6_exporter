    "configurations": [
        {
            "name": "Debug gddr6",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/gddr6",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
